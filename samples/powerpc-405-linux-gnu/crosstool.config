CT_LOCAL_TARBALLS_DIR="${HOME}/src"
CT_SAVE_TARBALLS=y
CT_LOG_EXTRA=y
CT_ARCH_CPU="405"
CT_ARCH_TUNE="405"
CT_ARCH_FLOAT_SW=y
CT_ARCH_powerpc=y
CT_TARGET_VENDOR="405"
CT_KERNEL_linux=y
CT_BINUTILS_PLUGINS=y
CT_BINUTILS_FOR_TARGET=y
CT_CC_LANG_CXX=y
# CT_CC_GCC_ENABLE_TARGET_OPTSPACE must not be set or gcc fails to build
# due to use of crtsaves functions in crtend.o
# CT_CC_GCC_ENABLE_TARGET_OPTSPACE is not set
CT_CC_GCC_DISABLE_PCH=y
CT_LIBC_EGLIBC_V_2_13=y
CT_EGLIBC_OPT_SIZE=y
# CT_LIBC_GLIBC_FORCE_UNWIND is not set
CT_LIBC_GLIBC_USE_PORTS=y
CT_LIBC_GLIBC_KERNEL_VERSION_CHOSEN=y
CT_LIBC_GLIBC_MIN_KERNEL_VERSION="2.6.16"
CT_GMP_V_5_0_1=y
CT_MPFR_V_3_0_1=y
